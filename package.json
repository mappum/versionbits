{
  "name": "versionbits",
  "version": "0.2.0",
  "description": "Track Bitcoin versionbits deployments (BIP9)",
  "main": "index.js",
  "scripts": {
    "test": "standard src/*.js test/*.js && nyc tape test/*.js | faucet && nyc report",
    "test-browser": "npm run build && test/browser.sh && npm run source",
    "build": "rm -rf lib && babel --presets es2015 src -d lib",
    "source": "rm -rf lib && ln -s src lib",
    "prepublish": "npm run build",
    "publish": "npm run source"
  },
  "keywords": [
    "webcoin",
    "bitcoin",
    "version",
    "versionbits",
    "deployments",
    "bip9"
  ],
  "author": "Matt Bell <mappum@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "blockchain-state": "^0.1.0",
    "clone": "^1.0.2",
    "dstate": "^0.1.1",
    "level-browserify": "^1.1.0",
    "object-assign": "^4.1.0",
    "old": "^0.1.3",
    "to2": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "faucet": "0.0.1",
    "memdown": "^1.2.2",
    "nyc": "^6.6.1",
    "standard": "^7.1.2",
    "tape": "^4.6.0",
    "zuul": "^3.10.1",
    "zuul-ngrok": "^4.0.0"
  }
}
